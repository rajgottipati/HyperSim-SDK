{
  "name": "@hypersim/sdk",
  "version": "1.0.0",
  "description": "Advanced TypeScript SDK for HyperEVM blockchain with AI-powered transaction simulation and cross-layer integration",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && rollup -c",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docs": "typedoc src/index.ts",
    "prepublishOnly": "npm run build",
    "example:basic": "ts-node examples/basic-usage.ts",
    "example:simulation": "ts-node examples/transaction-simulation.ts",
    "example:streaming": "ts-node examples/websocket-streaming.ts",
    "example:ai": "ts-node examples/ai-analysis.ts"
  },
  "keywords": [
    "hyperliquid",
    "hyperevm",
    "hypercore",
    "blockchain",
    "ethereum",
    "sdk",
    "typescript",
    "ai",
    "simulation",
    "defi",
    "trading",
    "websocket",
    "streaming"
  ],
  "author": {
    "name": "HyperSim SDK Team",
    "email": "sdk@hypersim.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hypersim/typescript-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/hypersim/typescript-sdk/issues"
  },
  "homepage": "https://hypersim.dev",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "dependencies": {
    "@types/node": "^20.0.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/ws": "^8.5.9",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "rollup": "^4.5.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.2"
  },
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/hypersim"
  },
  "sideEffects": false
}